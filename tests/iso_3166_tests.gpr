with "config/iso_3166_tests_config.gpr";
project Iso_3166_Tests is

   for Source_Dirs use ("src/", "config/");
   for Object_Dir use "obj/" & Iso_3166_Tests_Config.Build_Profile;
   for Create_Missing_Dirs use "True";
   for Exec_Dir use "bin";
   for Main use ("iso_3166_tests.adb");

   package Compiler is
      for Default_Switches ("Ada") use Iso_3166_Tests_Config.Ada_Compiler_Switches;
   end Compiler;

   package Binder is
      for Switches ("Ada") use ("-Es"); --  Symbolic traceback
   end Binder;

   package Install is
      for Artifacts (".") use ("share");
   end Install;

end Iso_3166_Tests;
